    {
      "type": "wokwi-resistor",
      "id": "resistor-display",
      "top": 134.45,
      "left": -163.75,
      "rotate": 0,
      "attrs": { "value": "1000" }
    },
    {
      "type": "wokwi-led",
      "id": "led-success",
      "top": 0,
      "left": -55,
      "attrs": { "color": "green", "flip": "1" }
    },
    {
      "type": "wokwi-resistor",
      "id": "resistor-success",
      "top": 75,
      "left": -42,
      "rotate": 90,
      "attrs": { "value": "1000" }
    },
    {
      "type": "wokwi-led",
      "id": "led-failure",
      "top": 0,
      "left": 100,
      "attrs": { "color": "red", "flip": "1" }
