      "rotate": 0,
      "attrs": { "color": "blue", "flip": "1" }
    },
    {
      "type": "wokwi-led",
      "id": "led-display",
      "top": 120,
      "left": -220,
      "rotate": -90,
      "attrs": { "color": "yellow", "flip": "1" }
